{
  "name": "tc-law-website",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "open-cli http://localhost:3000 && next dev",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "ts:check": "tsc --noEmit --incremental",
    "next:build": "next build",
    "next:start": "next start",
    "next:export": "next export",
    "next:b:start": "next build && next start",
    "next:b:export": "next build && next export",
    "b": "npm run bundle",
    "bundle": "bash ./scripts/bundle.sh",
    "gh-deploy": "bash ./scripts/gh_deploy.sh",
    "gh-deploy:next-export": "cross-env DEPLOY_ENV=github-pages-env npm run next:b:export",
    "gh-deploy:save-out-dir-to-pages": "gh-pages -d out --dotfiles",
    "zip:out": "cd ./out && zip -r ../tc-law-out.zip ./*",
    "zip:out:wrapped": "zip -r tc-law-out.zip ./out",
    "zip:out:full": "npm run next:b:export && npm run zip:out",
    "clean": "bash ./scripts/clean.sh",
    "install:npm": "npm i && npm i -D",
    "install:yarn": "yarn",
    "storybook": "npm run storybook:dev",
    "storybook:dev": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.2",
    "@react-hook/size": "^2.1.2",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "formik": "^2.2.9",
    "gray-matter": "^4.0.3",
    "hamburger-react": "^2.5.0",
    "markdown-to-txt": "^2.0.1",
    "next": "^12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^4.4.0",
    "react-markdown": "^8.0.3",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "sass": "^1.54.5",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@fontsource/montserrat": "^4.5.12",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/node": "18.7.13",
    "@types/react": "^18.0.17",
    "@types/react-google-recaptcha": "^2.1.5",
    "babel-loader": "^8.2.5",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-plugin-storybook": "^0.6.4",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "open-cli": "^7.0.1",
    "storybook-addon-next": "^1.6.7",
    "typescript": "4.8.2"
  }
}
