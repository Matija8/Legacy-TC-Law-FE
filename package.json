{
  "name": "tc-law-website",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "open-cli http://localhost:3000 && next dev",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "next:build": "next build",
    "next:start": "next start",
    "next:export": "next export",
    "next:b:start": "next build && next start",
    "next:b:export": "next build && next export",
    "gh-deploy": "npm run gh:predeploy && npm run gh:save-out-dir-to-pages",
    "gh:predeploy": "npm run gh:next-export && npm run gh:add-nojekyll",
    "gh:next-export": "cross-env DEPLOY_ENV=github-pages-env npm run next:b:export",
    "gh:add-nojekyll": "touch ./out/.nojekyll",
    "gh:save-out-dir-to-pages": "gh-pages -d out --dotfiles",
    "zip:all": "zip -r tc-law-bundle.zip .",
    "zip:out": "zip -r tc-law-out.zip ./out",
    "zip:out:full": "npm run next:b:export && npm run zip:out",
    "clean": "npm run clean:rm-next && npm run clean:rm-out && npm run clean:rm-packages",
    "clean:rm-next": "rm -rf .next",
    "clean:rm-out": "rm -rf out",
    "clean:rm-packages": "rm -rf node_modules && rm -f yarn.lock && rm -f package-lock.json",
    "install:npm": "npm i && npm i -D",
    "install:yarn": "yarn",
    "storybook": "npm run storybook:dev",
    "storybook:dev": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.0",
    "@react-hook/size": "^2.1.2",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "formik": "^2.2.9",
    "gray-matter": "^4.0.3",
    "hamburger-react": "^2.4.1",
    "markdown-to-txt": "^2.0.1",
    "next": "^12.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^4.3.1",
    "react-markdown": "^8.0.0",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "sass": "^1.45.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@fontsource/montserrat": "^4.5.10",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/node": "17.0.0",
    "@types/react": "^18.0.8",
    "@types/react-google-recaptcha": "^2.1.5",
    "babel-loader": "^8.2.5",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-plugin-storybook": "^0.5.11",
    "gh-pages": "^3.2.3",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "open-cli": "^7.0.1",
    "storybook-addon-sass-postcss": "^0.1.1",
    "typescript": "4.5.4"
  }
}
