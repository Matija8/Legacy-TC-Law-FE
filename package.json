{
  "name": "tc-law-website",
  "private": true,
  "scripts": {
    "dev": "open-cli http://localhost:3000 && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test-watch": "jest --watch",
    "export": "next build && next export",
    "gh-add-nojekyll": "touch ./out/.nojekyll",
    "gh-predeploy": "npm run export && npm run gh-add-nojekyll",
    "gh-deploy": "npm run gh-predeploy && gh-pages -d out --dotfiles",
    "rm-packages": "rm -rf node_modules && rm -f yarn.lock && rm -f package-lock.json",
    "install:npm": "npm i && npm i -D",
    "install:yarn": "yarn"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.0",
    "@react-hook/size": "^2.1.2",
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "gray-matter": "^4.0.3",
    "hamburger-react": "^2.4.1",
    "markdown-to-txt": "^2.0.1",
    "next": "^12.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-markdown": "^8.0.0",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "sass": "^1.45.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/node": "17.0.0",
    "@types/react": "17.0.37",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "gh-pages": "^3.2.3",
    "jest": "^27.5.1",
    "open-cli": "^7.0.1",
    "typescript": "4.5.4"
  }
}
